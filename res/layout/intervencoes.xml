<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#FFFFFF"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
		android:layout_width="match_parent"
		android:id="@+id/linear_principal"
		android:layout_height="wrap_content"
		android:orientation="vertical" >
		
         <TableLayout
                 android:layout_width="match_parent"
                 android:shrinkColumns="2"
                 android:layout_height="60dp" >

                 <TableRow
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginTop="10dp"
                     android:gravity="center" >
                     
                     <TextView
                     android:id="@+id/textView99"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:text="Intervenções"
                     android:textColor="#000000"
                     android:textSize="16sp" />
                     
                 </TableRow>

                 <TableRow
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:gravity="right" >
                     
                 </TableRow>
             </TableLayout>
             
         
         
         
         <TableLayout
                 android:layout_width="829dp"
                 android:shrinkColumns="3"
                 android:layout_gravity="center"
                 android:layout_height="wrap_content" >
            
                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp" >
                
    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_marginRight="200dp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_height="wrap_content"
        android:text="Remoção" />
    
    
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_marginRight="250dp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_height="wrap_content"
        android:text="Lona Plástica" />
    
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_height="wrap_content"
        android:text="Demolição" />
    
    
    </TableRow>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="80dp"
                    android:layout_marginTop="10dp"
                    android:gravity="left" >
                
    <RadioGroup
              android:id="@+id/rg_1_intervencoes"
              android:layout_width="wrap_content"
              android:orientation="horizontal"
              android:layout_height="wrap_content" >

              <RadioButton
                  android:id="@+id/radio0"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:textColor="#000000"
                  android:checked="true"
                  android:text="Sim" />

              <RadioButton
                  android:id="@+id/radio1"
                  android:layout_width="wrap_content"
                  android:textColor="#000000"
                  android:layout_height="wrap_content"
                  android:text="Não" />
          
          </RadioGroup>
    
                        <RadioGroup
              android:id="@+id/rg_2_intervencoes"
              android:layout_width="wrap_content"
              android:orientation="horizontal"
              android:layout_height="wrap_content" >

              <RadioButton
                  android:id="@+id/radio0"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:textColor="#000000"
                  android:checked="true"
                  android:text="Reposição" />

              <RadioButton
                  android:id="@+id/radio1"
                  android:layout_width="wrap_content"
                  android:textColor="#000000"
                  android:layout_height="wrap_content"
                  android:text="Colocalção" />
          
          </RadioGroup>
    
   
    
     <RadioGroup
              android:id="@+id/rg_2_intervencoes"
              android:layout_width="wrap_content"
              android:orientation="horizontal"
              android:layout_height="wrap_content" >

              <RadioButton
                  android:id="@+id/radio0"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:textColor="#000000"
                  android:checked="true"
                  android:text="Parcial" />

              <RadioButton
                  android:id="@+id/radio1"
                  android:layout_width="wrap_content"
                  android:textColor="#000000"
                  android:layout_height="wrap_content"
                  android:text="Total" />
          
          </RadioGroup>
    
    
    </TableRow>
    
                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp" >
                
    <TextView
        android:id="@+id/textView25454"
        android:layout_width="wrap_content"
        android:layout_marginRight="200dp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_height="wrap_content"
        android:text="Árvore" />
    
    
    <TextView
        android:id="@+id/textView33232"
        android:layout_width="wrap_content"
        android:layout_marginRight="250dp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_height="wrap_content"
        android:text="Auxílio Moradia" />
    
    
    </TableRow>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="80dp"
                    android:layout_marginTop="10dp"
                    android:gravity="left" >
                
    <RadioGroup
              android:id="@+id/rg_4_intervencoes"
              android:layout_width="wrap_content"
              android:orientation="horizontal"
              android:layout_height="wrap_content" >

              <RadioButton
                  android:id="@+id/radio1212"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:textColor="#000000"
                  android:checked="true"
                  android:text="Erradicação" />

              <RadioButton
                  android:id="@+id/radio13323"
                  android:layout_width="wrap_content"
                  android:textColor="#000000"
                  android:layout_height="wrap_content"
                  android:text="Podação" />
          
          </RadioGroup>
    
                        <RadioGroup
              android:id="@+id/rg_5_intervencoes"
              android:layout_width="wrap_content"
              android:orientation="horizontal"
              android:layout_height="wrap_content" >

              <RadioButton
                  android:id="@+id/radio656"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:textColor="#000000"
                  android:checked="true"
                  android:text="Sim" />

              <RadioButton
                  android:id="@+id/radio12323"
                  android:layout_width="wrap_content"
                  android:textColor="#000000"
                  android:layout_height="wrap_content"
                  android:text="Não" />
          
          </RadioGroup>
    
   
    
    
    
    
    </TableRow>
    
                <TableRow
                     android:layout_width="wrap_content"
                     android:layout_height="80dp"
                     android:layout_marginTop="50dp"
                     android:gravity="left" >
         
                <TextView
        android:id="@+id/textView33212122"
        android:layout_width="wrap_content"
        android:layout_marginRight="250dp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_height="wrap_content"
        android:text="Outros" />
         
                </TableRow>
     
    </TableLayout>
                
           <LinearLayout
               android:layout_width="818dp"
               android:layout_height="match_parent"
               android:layout_marginLeft="62dp"
               android:orientation="vertical" >

               <EditText
                   android:id="@+id/editText1"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:ems="10" >

                   <requestFocus />
               </EditText>
               
           </LinearLayout>   
                
                 
                 
          <LinearLayout
            android:layout_width="wrap_content"
            android:layout_marginTop="50dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            
             >
            <Button 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/bt_finalizar_intervencoes"
                android:text="Finalizar"
                />
            
            
        </LinearLayout>
         
         
         
         
         
         
         
         
         
         
         
    

</LinearLayout>
</ScrollView>